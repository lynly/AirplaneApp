              <ul class="right hide-on-med-and-down">
                <li><%= link_to "Airplanes", "/airplanes" %>Airplanes</li>
                <li><%= link_to "Flights", "/flights" %>Flights</li>
                <li><%= link_to "Users", "/users" %>Users</li>
                <li><%= link_to "Reservations", "/reservations" %>Reservations</li>
                <li>
                  <% if @current_user %>
                    <%= link_to @current_user.email, user_path(@current_user) %>
                  <% else %>
                    <%= link_to 'Login', '/login' %>
                  <% end %>
                  <li>

                  <li>
                    <% if @current_user %>
                      <%= link_to "Log Out", "/logout", method: :delete %>
                    <% else %>
                      <%= link_to "Sign Up", "/users/new" %>
                    <% end %>
                </li>
              </ul>
            </div>

    </nav>

    <div class="main">
      <div class="container">

      <%= yield %>

      </div>
    </div>

